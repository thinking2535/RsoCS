include <Rso/Net/Protocol>

namespace rso
{
	namespace monitor
	{
		using namespace std
		using namespace net

		struct SCommand
		{
			wstring Name
			wstring Command
		}
		struct SServer : SNamePort
		{
			wstring ServerName
		}
		struct SOption
		{
			wstring ID
			wstring PW
			int32 ServerNo
			vector<SServer> Servers
			wstring LocalDirectory
			wstring RemoteDirectory
			vector<SCommand> Commands
		}
	}
}
